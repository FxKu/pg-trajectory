SELECT t1.id AS traj_1_ID, t2.id AS traj_2_ID, EuclideanDistance(t1.tr, t2.tr) AS EuclideanDistance
FROM small_test_traj_both t1, small_test_traj_both t2
WHERE (t1.tr).geom_type = 'Point' AND (t2.tr).geom_type = 'Point';

SELECT t1.id AS traj_1_ID, t2.id AS traj_2_ID, EuclideanDistance(t1.tr, t2.tr) AS EuclideanDistance
FROM small_test_traj_both t1, small_test_traj_both t2
WHERE (t1.tr).geom_type = 'Polygon' AND (t2.tr).geom_type = 'Polygon';

SELECT t1.id, (t1.tr).geom_type FROM small_test_traj_both t1;


SELECT zNormalize((t.tr).tr_data,1,4) FROM small_test_traj_both t WHERE t.id = 4;

SELECT t.id, findTimeLength(t.tr)
FROM solar_region_traj_26 t;

--both equal length 161 -- 1.44734197458954
SELECT t1.id AS traj_1_ID, t2.id AS traj_2_ID, EuclideanDistance(t1.tr, t2.tr) AS EuclideanDistance
FROM solar_region_traj_26 t1, solar_region_traj_26 t2
WHERE t1.id = 4001854 AND t2.id = 4001850;

--big length difference 21 and -- 9.16515138990391
SELECT t1.id AS traj_1_ID, t2.id AS traj_2_ID, EuclideanDistance(t1.tr, t2.tr) AS EuclideanDistance
FROM solar_region_traj_26 t1, solar_region_traj_26 t2
WHERE t1.id = 2000926 AND t2.id = 4001855;